    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Profiler",
            "type": "python",
            "request": "launch",
            "module": "cProfile",
            "args":[
                "-o", "${workspaceFolder}/tmp.prof", "${workspaceFolder}/sac_continuous_action.py"
            ]
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/ppo_continuous_action.py",
            "console": "integratedTerminal",
            "justMyCode": false
        }
    ]
}